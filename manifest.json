{
  "name": "CS:GO Skin Tester",
  "version": "1.0",
  "description": "Test skins in-game",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "tabs",
    "notifications",
    "https://api.csgoskins.gg/*"
  ],
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  }
}
